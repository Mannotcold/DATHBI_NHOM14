  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>SELECT
    DL.[Region],
    D.[Order Date],
    SUM(FO.[Sales]) AS TongDoanhThu
FROM
    [Fact_Order] FO
JOIN
    [Dim_Location] DL ON FO.[Location ID_SK] = DL.[Location ID_SK]
JOIN
    [Dim_Date] D ON FO.[Date ID_SK] = D.[Date ID_SK]
GROUP BY
    DL.[Region],
    D.[Order Date];</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="Region">
          <DataField>Region</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Order_Date">
          <DataField>Order Date</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="TongDoanhThu">
          <DataField>TongDoanhThu</DataField>
          <rd:TypeName>System.Double</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
